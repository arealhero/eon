fibonacci: (n: mutable Int32) -> Int32 =
{
    prev: mutable _ = 0;
    cur: mutable _ = 1;

    while n != 0
    {
        sum := prev + cur;
        prev = cur;
        cur = sum;

        n = n - 1;
    }

    return prev;
}

main: () -> Int32 =
{
    print(fibonacci(12)); // NOTE(vlad): 144
    return 0;
}
